

<!-- MAIN CONTENT -->

            <div class="col-xs-12 row listOfMyLists">
                <p class="list_title">Your Lists</p>
                <ul id="my-list-list">
                  <% @populists.each do |populist| %>
                        <li><%= link_to populist.title, populist_path(populist) %>
                          <div class="visibility-status">
                            <% if populist.public == true %>
                              <span class="glyphicon glyphicon-eye-open eye" aria-hidden="true"></span>
                            <% else %>
                            <span class="glyphicon glyphicon-eye-close eye" aria-hidden="true"></span>
                          <%  end  %>

                          <%= link_to " ", populist, method: :delete, data: {confirm: "Are you sure?"}, :class => "glyphicon glyphicon-trash" %>

                        </div>
                      </li>
                    <% end %>


                      <li><%= link_to "New Lists", new_populist_path %><div class="visibility-status"><span class="glyphicon glyphicon-plus create-new-plus eye" aria-hidden="true"></span></div></li>

                </ul>


            </div><!--listOfMyLists-->

